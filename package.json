{
  "name": "vite-react-mobx-ssr",
  "type": "module",
  "private": true,
  "scripts": {
    "prepare": "sh simple-git-hooks.sh",
    "clean": "rm -rf ./dist",
    "serve": "vite --mode development --config config/vite.dev.ts",
    "build:prod": "vite build --mode production --config config/vite.prod.ts",
    "build:server": "vite build --mode production --config config/vite.server.ts",
    "build:serverEntry": "vite build --mode production --config config/vite.serverEntry.ts",
    "build": "npm run clean & npm run build:prod && npm run build:server && npm run build:serverEntry",
    "start": "node ./dist/index.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:css": "stylelint \"**/*.{css,scss,sass}\" --fix",
    "lint:ts": "tsc --noEmit"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.17.0",
    "antd": "^5.9.3",
    "axios": "^1.4.0",
    "express": "^4.18.2",
    "md5": "^2.3.0",
    "mobx": "^6.10.0",
    "mobx-react-lite": "^4.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.15.0",
    "react-router-dom": "^6.15.0",
    "serialize-javascript": "^6.0.1"
  },
  "devDependencies": {
    "@lincy/base-config": "^0.0.2",
    "@lincy/eslint-config": "^3.1.0",
    "@lincy/eslint-config-react": "^2.1.0",
    "@lincy/stylelint-config": "^1.15.10",
    "@lincy/vite-plugin-mock": "^3.0.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.0",
    "@types/postcss-flexbugs-fixes": "^5.0.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/serialize-javascript": "^5.0.2",
    "@unocss/eslint-config": "^0.55.0",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-react": "^4.0.4",
    "ahooks": "^3.7.8",
    "eslint": "^8.47.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.0",
    "lru-cache": "^10.0.1",
    "postcss": "^8.4.27",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^9.1.1",
    "sass": "^1.65.1",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^15.10.2",
    "typescript": "^5.1.6",
    "unocss": "^0.56.4",
    "unplugin-auto-import": "^0.16.6",
    "vite": "^4.4.9",
    "vite-plugin-importus": "^1.0.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --fix",
    "*.{css,scss}": "stylelint --fix"
  }
}
