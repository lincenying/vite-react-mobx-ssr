{
  "name": "vite-react-mobx-ssr",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.23.0",
  "scripts": {
    "prepare[-del]": "npx simple-git-hooks",
    "clean": "rm -rf ./dist",
    "serve": "vite --mode development --config vite.config.dev.ts",
    "build:prod": "vite build --mode production --config vite.config.prod.ts",
    "build:server": "vite build --mode production --config vite.config.server.ts",
    "build:node": "vite build --mode production --config vite.config.node.ts",
    "build:css": "esno creatAntdCss.ts",
    "build": "npm run clean & npm run build:prod && npm run build:server && npm run build:node && npm run build:css",
    "start": "node ./dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:css": "stylelint \"**/*.{css,scss,sass}\" --fix",
    "lint:ts": "tsc -b --noEmit"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^2.0.1",
    "antd": "^6.0.0",
    "axios": "^1.13.2",
    "express": "^5.1.0",
    "md5": "^2.3.0",
    "mobx": "^6.15.0",
    "mobx-react-lite": "^4.1.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.6",
    "serialize-javascript": "^7.0.0",
    "store2": "^2.14.4"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^2.3.7",
    "@lincy/eslint-config": "^6.0.1",
    "@lincy/static-style-extract": "^1.0.5",
    "@lincy/stylelint-config": "^2.2.0",
    "@lincy/unocss-base-config": "^2.3.0",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/postcss-flexbugs-fixes": "^5.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/serialize-javascript": "^5.0.4",
    "@unocss/eslint-config": "^66.5.9",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-react": "^5.1.1",
    "ahooks": "^3.9.6",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-mobx": "^0.0.13",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "esno": "^4.8.0",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.7",
    "lru-cache": "^11.2.2",
    "postcss": "^8.5.6",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^10.4.0",
    "sass": "^1.94.2",
    "stylelint": "^16.26.0",
    "typescript": "^5.9.3",
    "unocss": "^66.5.9",
    "unplugin-auto-import": "^20.2.0",
    "vite": "^7.2.4",
    "vite-plugin-mock": "^3.0.2"
  },
  "simple-git-hooks[-del]": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged[-del]": {
    "*.{js,ts,jsx,tsx}": "eslint --fix",
    "*.{css,scss}": "stylelint --fix --allow-empty-input"
  }
}
