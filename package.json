{
  "name": "vite-react-mobx-ssr",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "prepare": "npx simple-git-hooks",
    "clean": "rm -rf ./dist",
    "serve": "vite --mode development --config vite.config.dev.ts",
    "build:prod": "vite build --mode production --config vite.config.prod.ts",
    "build:server": "vite build --mode production --config vite.config.server.ts",
    "build:node": "vite build --mode production --config vite.config.node.ts",
    "build:css": "esno creatAntdCss.ts",
    "build": "npm run clean & npm run build:prod && npm run build:server && npm run build:node && npm run build:css",
    "start": "node ./dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:css": "stylelint \"**/*.{css,scss,sass}\" --fix",
    "lint:ts": "tsc --noEmit"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.23.0",
    "antd": "^5.25.1",
    "axios": "^1.9.0",
    "express": "^5.1.0",
    "md5": "^2.3.0",
    "mobx": "^6.13.7",
    "mobx-react-lite": "^4.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^6.30.0",
    "react-router-dom": "^6.30.0",
    "serialize-javascript": "^6.0.2",
    "store2": "^2.14.4"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.49.0",
    "@lincy/eslint-config": "^5.5.2",
    "@lincy/static-style-extract": "^1.0.5",
    "@lincy/stylelint-config": "^2.0.1",
    "@lincy/unocss-base-config": "^2.2.1",
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.18",
    "@types/postcss-flexbugs-fixes": "^5.0.3",
    "@types/react": "^18.3.21",
    "@types/react-dom": "^18.3.7",
    "@types/serialize-javascript": "^5.0.4",
    "@unocss/eslint-config": "^66.1.2",
    "@vitejs/plugin-legacy": "^6.1.1",
    "@vitejs/plugin-react": "^4.4.1",
    "ahooks": "^3.8.4",
    "cross-env": "^7.0.3",
    "eslint": "^9.26.0",
    "eslint-plugin-mobx": "^0.0.13",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "esno": "^4.8.0",
    "jiti": "^2.4.2",
    "lint-staged": "^16.0.0",
    "lru-cache": "^11.1.0",
    "postcss": "^8.5.3",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^10.1.6",
    "sass": "^1.88.0",
    "simple-git-hooks": "^2.13.0",
    "stylelint": "^16.19.1",
    "typescript": "^5.8.3",
    "unocss": "^66.1.2",
    "unplugin-auto-import": "^19.2.0",
    "vite": "^6.3.5",
    "vite-plugin-mock": "^3.0.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --fix",
    "*.{css,scss}": "stylelint --fix --allow-empty-input"
  }
}
