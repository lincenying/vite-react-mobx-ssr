{
  "name": "vite-react-mobx-ssr",
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "prepare": "husky install",
    "serve": "vite --mode development --config config/vite.dev.ts",
    "build:prod": "vite build --mode production --config config/vite.prod.ts",
    "build:server": "vite build --mode production --config config/vite.server.ts",
    "build:serverEntry": "vite build --mode production --config config/vite.serverEntry.ts",
    "start": "node ./dist/server.js",
    "clean": "rm -rf ./dist",
    "lint": "eslint . --ext .vue,.js,.ts,.tsx",
    "lint:fix": "eslint . --ext .vue,.js,.ts,.tsx --fix",
    "lint:css": "stylelint \"**/*.{css,scss,sass}\" --fix",
    "lint:ts": "vue-tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "express": "^4.18.2",
    "isomorphic-fetch": "^3.0.0",
    "md5": "^2.3.0",
    "mobx": "^6.10.0",
    "mobx-react-lite": "^4.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.14.2",
    "react-router-dom": "^6.14.2",
    "serialize-javascript": "^6.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.5",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@lincy/eslint-config": "^2.1.0",
    "@lincy/eslint-config-react": "^2.1.0",
    "@lincy/stylelint-config": "^1.15.10",
    "@lincy/vite-plugin-mock": "^3.0.0",
    "@types/express": "^4.17.17",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/node": "^20.4.9",
    "@types/postcss-flexbugs-fixes": "^5.0.0",
    "@types/react": "^18.2.19",
    "@types/react-dom": "^18.2.7",
    "@types/serialize-javascript": "^5.0.2",
    "@unocss/eslint-config": "^0.54.2",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-react": "^4.0.4",
    "ahooks": "^3.7.8",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.46.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "lru-cache": "^10.0.0",
    "postcss": "^8.4.27",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^9.1.1",
    "sass": "^1.64.2",
    "stylelint": "^15.10.2",
    "typescript": "^5.1.6",
    "vite": "^4.4.9"
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx}": "eslint --cache --fix",
    "*.{css,scss}": "stylelint --fix"
  }
}
