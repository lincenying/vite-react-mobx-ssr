{
  "name": "vite-react-mobx-ssr",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.1.0",
  "scripts": {
    "prepare": "npx simple-git-hooks",
    "clean": "rm -rf ./dist",
    "serve": "vite --mode development --config vite.config.dev.ts",
    "build:prod": "vite build --mode production --config vite.config.prod.ts",
    "build:server": "vite build --mode production --config vite.config.server.ts",
    "build:node": "vite build --mode production --config vite.config.node.ts",
    "build:css": "esno creatAntdCss.ts",
    "build": "npm run clean & npm run build:prod && npm run build:server && npm run build:node && npm run build:css",
    "start": "node ./dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:css": "stylelint \"**/*.{css,scss,sass}\" --fix",
    "lint:ts": "tsc --noEmit"
  },
  "dependencies": {
    "@ant-design/cssinjs": "1.21.1",
    "antd": "^5.20.6",
    "axios": "^1.7.7",
    "express": "^4.21.0",
    "md5": "^2.3.0",
    "mobx": "^6.13.2",
    "mobx-react-lite": "^4.0.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^6.26.2",
    "react-router-dom": "^6.26.2",
    "serialize-javascript": "^6.0.2",
    "store2": "^2.14.3"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.14.1",
    "@lincy/base-config": "^1.3.0",
    "@lincy/eslint-config": "^5.2.4",
    "@lincy/static-style-extract": "^1.0.5",
    "@lincy/stylelint-config": "^2.0.0",
    "@lincy/vite-plugin-mock": "^3.0.0",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.5",
    "@types/postcss-flexbugs-fixes": "^5.0.3",
    "@types/react": "^18.3.7",
    "@types/react-dom": "^18.3.0",
    "@types/serialize-javascript": "^5.0.4",
    "@unocss/eslint-config": "^0.62.4",
    "@vitejs/plugin-legacy": "^5.4.2",
    "@vitejs/plugin-react": "^4.3.1",
    "ahooks": "^3.8.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.10.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.10",
    "lru-cache": "^11.0.1",
    "postcss": "^8.4.47",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^10.0.3",
    "sass": "^1.78.0",
    "simple-git-hooks": "^2.11.1",
    "stylelint": "^16.9.0",
    "typescript": "^5.6.2",
    "unocss": "^0.62.4",
    "unplugin-auto-import": "^0.18.3",
    "vite": "^5.4.6"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --fix",
    "*.{css,scss}": "stylelint --fix --allow-empty-input"
  }
}
