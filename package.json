{
  "name": "vite-react-mobx-ssr",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "npx simple-git-hooks",
    "clean": "rm -rf ./dist",
    "serve": "vite --mode development --config vite.config.dev.ts",
    "build:prod": "vite build --mode production --config vite.config.prod.ts",
    "build:server": "vite build --mode production --config vite.config.server.ts",
    "build:node": "vite build --mode production --config vite.config.node.ts",
    "build:css": "esno creatAntdCss.ts",
    "build": "npm run clean & npm run build:prod && npm run build:server && npm run build:node && npm run build:css",
    "start": "node ./dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:css": "stylelint \"**/*.{css,scss,sass}\" --fix",
    "lint:ts": "tsc --noEmit"
  },
  "dependencies": {
    "@ant-design/cssinjs": "1.18.5",
    "antd": "^5.15.4",
    "axios": "^1.6.8",
    "express": "^4.19.2",
    "md5": "^2.3.0",
    "mobx": "^6.12.1",
    "mobx-react-lite": "^4.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.22.3",
    "react-router-dom": "^6.22.3",
    "serialize-javascript": "^6.0.2",
    "store2": "^2.14.3"
  },
  "devDependencies": {
    "@lincy/base-config": "^1.2.0",
    "@lincy/eslint-config": "^4.2.6",
    "@lincy/static-style-extract": "^1.0.5",
    "@lincy/stylelint-config": "^1.17.4",
    "@lincy/vite-plugin-mock": "^3.0.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "@types/postcss-flexbugs-fixes": "^5.0.3",
    "@types/react": "^18.2.72",
    "@types/react-dom": "^18.2.22",
    "@types/serialize-javascript": "^5.0.4",
    "@unocss/eslint-config": "^0.58.7",
    "@vitejs/plugin-legacy": "^5.3.2",
    "@vitejs/plugin-react": "^4.2.1",
    "ahooks": "^3.7.10",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-format": "^0.1.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.2",
    "lru-cache": "^10.2.0",
    "postcss": "^8.4.38",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^9.5.2",
    "sass": "^1.72.0",
    "simple-git-hooks": "^2.11.1",
    "stylelint": "^16.3.1",
    "typescript": "^5.4.3",
    "unocss": "^0.58.7",
    "unplugin-auto-import": "^0.17.5",
    "vite": "^5.2.6"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --fix",
    "*.{css,scss}": "stylelint --fix --allow-empty-input"
  }
}
