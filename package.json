{
  "name": "vite-react-mobx-ssr",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.14.2",
  "scripts": {
    "prepare": "npx simple-git-hooks",
    "clean": "rm -rf ./dist",
    "serve": "vite --mode development --config vite.config.dev.ts",
    "build:prod": "vite build --mode production --config vite.config.prod.ts",
    "build:server": "vite build --mode production --config vite.config.server.ts",
    "build:node": "vite build --mode production --config vite.config.node.ts",
    "build:css": "esno creatAntdCss.ts",
    "build": "npm run clean & npm run build:prod && npm run build:server && npm run build:node && npm run build:css",
    "start": "node ./dist/index.js",
    "lint": "eslint . --flag unstable_ts_config",
    "lint:fix": "eslint . --flag unstable_ts_config --fix",
    "lint:css": "stylelint \"**/*.{css,scss,sass}\" --fix",
    "lint:ts": "tsc --noEmit"
  },
  "dependencies": {
    "@ant-design/cssinjs": "1.21.1",
    "antd": "^5.22.2",
    "axios": "^1.7.8",
    "express": "^4.21.1",
    "md5": "^2.3.0",
    "mobx": "^6.13.5",
    "mobx-react-lite": "^4.0.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.0.1",
    "react-router-dom": "^7.0.1",
    "serialize-javascript": "^6.0.2",
    "store2": "^2.14.3"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.17.1",
    "@lincy/base-config": "^1.3.3",
    "@lincy/eslint-config": "^5.3.0",
    "@lincy/static-style-extract": "^1.0.5",
    "@lincy/stylelint-config": "^2.0.1",
    "@lincy/vite-plugin-mock": "^3.0.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.9.4",
    "@types/postcss-flexbugs-fixes": "^5.0.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/serialize-javascript": "^5.0.4",
    "@unocss/eslint-config": "^0.64.1",
    "@vitejs/plugin-legacy": "^5.4.3",
    "@vitejs/plugin-react": "^4.3.3",
    "ahooks": "^3.8.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.15.0",
    "eslint-plugin-mobx": "^0.0.13",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "esno": "^4.8.0",
    "jiti": "^2.4.0",
    "lint-staged": "^15.2.10",
    "lru-cache": "^11.0.2",
    "postcss": "^8.4.49",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^10.1.1",
    "sass": "^1.81.0",
    "simple-git-hooks": "^2.11.1",
    "stylelint": "^16.10.0",
    "typescript": "^5.6.3",
    "unocss": "^0.64.1",
    "unplugin-auto-import": "^0.18.5",
    "vite": "^5.4.11"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --flag unstable_ts_config --fix",
    "*.{css,scss}": "stylelint --fix --allow-empty-input"
  }
}
