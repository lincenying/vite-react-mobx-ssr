{
  "name": "vite-react-mobx-ssr",
  "type": "module",
  "private": true,
  "scripts": {
    "prepare": "sh simple-git-hooks.sh",
    "clean": "rm -rf ./dist",
    "serve": "vite --mode development --config config/vite.dev.ts",
    "build:prod": "vite build --mode production --config config/vite.prod.ts",
    "build:server": "vite build --mode production --config config/vite.server.ts",
    "build:serverEntry": "vite build --mode production --config config/vite.serverEntry.ts",
    "build": "npm run clean & npm run build:prod && npm run build:server && npm run build:serverEntry",
    "start": "node ./dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:css": "stylelint \"**/*.{css,scss,sass}\" --fix",
    "lint:ts": "tsc --noEmit",
    "antd": "ts-node --project ./tsconfig.node.json ./scripts/genAntdCss.tsx"
  },
  "dependencies": {
    "@ant-design/cssinjs": "1.18.0-alpha.5",
    "antd": "^5.11.5",
    "axios": "^1.6.2",
    "express": "^4.18.2",
    "md5": "^2.3.0",
    "mobx": "^6.12.0",
    "mobx-react-lite": "^4.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.20.0",
    "react-router-dom": "^6.20.0",
    "serialize-javascript": "^6.0.1"
  },
  "devDependencies": {
    "@ant-design/static-style-extract": "^1.0.2",
    "@lincy/base-config": "^1.0.1",
    "@lincy/eslint-config": "^4.1.1",
    "@lincy/eslint-config-react": "^2.5.4",
    "@lincy/static-style-extract": "^1.0.3",
    "@lincy/stylelint-config": "^1.15.10",
    "@lincy/vite-plugin-mock": "^3.0.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.1",
    "@types/postcss-flexbugs-fixes": "^5.0.3",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@types/serialize-javascript": "^5.0.4",
    "@unocss/eslint-config": "^0.57.7",
    "@vitejs/plugin-legacy": "^5.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "ahooks": "^3.7.8",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "lru-cache": "^10.1.0",
    "postcss": "^8.4.31",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^9.3.0",
    "sass": "^1.69.5",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^15.11.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2",
    "unocss": "^0.57.7",
    "unplugin-auto-import": "^0.17.1",
    "vite": "^5.0.4",
    "vite-plugin-importus": "^1.0.5"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --fix",
    "*.{css,scss}": "stylelint --fix"
  }
}
